    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Harvester",
            "program": "${workspaceFolder}/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Index Cleaner",
            "program": "${workspaceFolder}/libs/index_tools/index_cleaner.js",
            "args": [
                "status",
                "0"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Index Terms",
            "outputCapture": "std",
            "program": "${workspaceFolder}/scripts/index/term/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Index Repos",
            "outputCapture": "std",
            "program": "${workspaceFolder}/scripts/index/repo/index.js"
        }
    ]
}